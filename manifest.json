{
	"manifest_version": 2,
	"name": "BostonText",
	"version": "0.0.1",
	"default_locale": "en",
	"description": "Replaces ordinary text with a Boston accent.",
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": ["bostontext.js"],
			"run_at": "document_end"
		}
	]
}